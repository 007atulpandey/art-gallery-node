       <div style=" text-align:center ; height:100%; min-width:100%;">
           
        <div class="col-md-12 px-5 py-5" style="min-height: 100%;width: 100%; background-color: gray">
            <h2 style="color: darkblue">your profile</h2>
            <center>
                <div style="border-radius:30px;">
                    <img height="60px" width="60px" src="https://cdn1.iconfinder.com/data/icons/navigation-elements/512/user-login-man-human-body-mobile-person-512.png" alt="user">
                  </div>        
            </center>
                      <h5 class="mt-3">
                          <strong>NAME : </strong>  <%=current.username %>
                      </h5>
                      <h5 class="mt-3">
                          <strong>Total Posts : </strong>  <%=img.length%>
                      </h5>
                       
                       
            
                    
        
        </div>
       
            
           
     

       </div>


 
        



       <h1>Your Posts</h1>
 <% p=0 %>
      <% img.forEach(function(i){%>
       <% if(p==0){ %>
        <div data-aos="fade-up-left" data-aos-duration="600" class="card mb-3" style="max-width: 600px;">
            <div class="row no-gutters">
              <div class="col-md-5">
                <img src="<%=i.link %>" style="height: 100%" class="card-img" alt="...">
              </div>
              <div class="col-md-7">
                <div class="card-body">
                  <h5 class="card-title"><%=i.title %></h5>
                  <p class="card-text"><%= i.description %></p>
                  <p class="card-text"><small class="text-muted">Last updated 3 mins ago</small></p>
                </div>
              </div>
            </div>
          </div>
         
       <% } %>
       <% if(p==1){ %>
        <div data-aos="fade-up-right" data-aos-duration="600" class="card mb-3" style="max-width: 600px;">
            <div class="row no-gutters">
              <div class="col-md-5">
                <img src="<%=i.link %>" style="height: 100%" class="card-img" alt="...">
              </div>
              <div class="col-md-7">
                <div class="card-body">
                  <h5 class="card-title"><%=i.title %></h5>
                  <p class="card-text"><%= i.description %></p>
                  <p class="card-text"><small class="text-muted">Last updated 3 mins ago</small></p>
                </div>
              </div>
            </div>
          </div>
         
       <% } %>
       <%p=p^1%>
          <% });%>








